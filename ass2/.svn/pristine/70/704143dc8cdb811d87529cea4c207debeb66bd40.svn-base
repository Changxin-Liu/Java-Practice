package pacman.ghost;

/**
 * GhostType contains all the valid ghosts; the current list of ghosts
 * are: "BLINKY", "CLYDE", "INKY", "PINKY".
 *
 * @ass1
 */
public enum GhostType {
    /**
     * Aggressive red ghost.
     */
    BLINKY,
    /**
     * Scared orange ghost.
     */
    CLYDE,
    /**
     * Following blue ghost.
     */
    INKY,
    /**
     * Ambushing pink ghost.
     */
    PINKY
}
