package pacman.ghost;

/**
 * A specified ghost called "Inky" with the give colour and type
 */
public class Inky extends Ghost{

    public Inky(){
        super();
    }

    /**
     * Returns the colour of Inky
     *
     * @return Inky's colour
     */
    public String getColour(){
        return "#7aa6da";
    }

    /**
     * Returns the type of Inky
     *
     * @return Inky's type
     */
    public GhostType getType(){
        return GhostType.INKY;
    }
}
