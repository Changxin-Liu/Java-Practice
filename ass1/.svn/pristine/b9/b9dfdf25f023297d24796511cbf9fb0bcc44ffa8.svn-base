package pacman.hunter;

/**
 * A specified hunter who is a mortal
 */
public class Phil extends Hunter {

    public Phil(){
        super();
    }

    public Phil(Hunter original){
        super(original);
    }

    /**
     * Since Phil is a mortal, his special will never be activated.
     * This means he can only do as a normal hunter.
     *
     * @return false means his special will never be activated
     */
    @Override
    public boolean isSpecialActive() {
        return false;
    }
}
